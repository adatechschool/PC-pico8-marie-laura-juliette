function collision(a,b)  
	if a.x>b.x+16
	or a.y>b.y+16
	or a.x+16<b.x
	or a.y+16<b.y then
		return false
	else
		return true
	end
end 

function grab()
	for i,t in pairs(trashtab) do	
 	if collision(p,t)==true	
 	and p.hastrash==0
 	then
 		p.hastrash = i
 	elseif p.hastrash==i 
 	then
 		t.x = p.x+5
 		t.y = p.y-5		
		end
	end
end


function shoottrash()	
	if p.hastrash>0 then
	trashtab[p.hastrash].y-=50
	p.hastrash = 0
	end
end
