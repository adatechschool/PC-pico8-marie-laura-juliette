--explosion
function create_explosions(x,y)
	add(explosions,{
		x = x,
		y = y,
		timer = 0
	})
	sfx(0)
	end
	
function update_explosions()
	for boom in all(explosions) do
	boom.timer +=1
		if boom.timer == 13 then
			del(explosions,boom)
		end
	end
end

function draw_explosions()
	for boom in all(explosions) do
		circ(boom.x, boom.y,
		 boom.timer/1.5, 8+boom.timer%3)
	end
end	  


