__lua__
function _init()
	p={x=60,y=110,speed=2}
	j={}
	v={}
end

function _update()
	if (btn(➡️)) p.x+=p.speed
	if (btn(⬅️)) p.x-=p.speed
	if (btn(⬆️)) p.y-=p.speed
	if (btn(⬇️)) p.y+=p.speed
end

function _draw()
	cls()
	
	map(0,0,0,0,128,64)
	spr(6,p.x,p.y)
	spr(8,30,30,2,2)
	spr(12,90,30,2,2)
end
