--collision-t-bin
function collision_bin(t,b)  
	if t.x>b.x+32
	or t.y>b.y+32
	or t.x+32<b.x
	or t.y+32<b.y then
		return false
	else
		return true
	end
end 

function update_trash1()
	for i,t in pairs(trashtab) do
		if trashtab[i].number==1
		and collision_bin(t,green_bin)
		then
		sfx(1)
		create_explosions(t.x,t.y)
		del (trashtab,trashtab[i])
		erreurs +=1
		end
	end
end				

function update_trash2()			
		for i,t in pairs(trashtab) do
			if trashtab[i].number==2
			and collision_bin(t,yellow_bin) 
			then
			sfx(1)
			create_explosions(t.x,t.y)
			del (trashtab,trashtab[i])
			erreurs +=1
		end
	end
end

function update_trash3()
	for i,t in pairs(trashtab) do
		if trashtab[i].number==2
		and collision_bin(t,green_bin)
		then
		sfx(0)
		create_flower(t.x,t.y)
		del (trashtab,trashtab[i])
		succes +=1
		end
	end
end	

function update_trash4()			
		for i,t in pairs(trashtab) do
			if trashtab[i].number==1
			and collision_bin(t,yellow_bin) 
			then
			sfx(0)
		 create_flower(t.x,t.y)
			del (trashtab,trashtab[i])
			succes +=1
		end
	end
end

function game_over()
	if erreurs == 3 then
	state = 1
	elseif succes == 10 then
	state = 2
	end
end
