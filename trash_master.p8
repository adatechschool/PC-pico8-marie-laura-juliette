--trump

function spawn_trumps(amount)
	gap=(128-8*amount)/(amount+1)
	for i=1,amount do
	add(trumps,{
		x=gap*i + 3*(i-1),
		y=-24,
	})
	end
end

function update_trumps()
	for t in all(trumps) do
	t.y+=0.3
		if t.y>128 then
		del (trumps,t)
		end
--collision
	for t in all(trumps) do
			if collision(t,p) then
			create_explosion(t.x+4,t.y+2)
			state=1
			end
		end
	end
end
