--collision
function collision(p,trash)
for trashCount=1,11 do
	if p.x>trash.x[trashCount]+16
	or p.y>trash.y[trashCount]+16
	or p.x+16<trash.x[trashCount]
	or p.y+16<trash.y[trashCount] then
		return false
	else
		return true
	end
end
end 

function grab(p,trash)
for trashCount=1,11 do
 if collision (p,trash) then
 return true 
 else 
 return false 
end 
end 
end

function hasTrash()
if grab()==true then  
 trash.x[trashCount] = p.x+5
 trash.y[trashCount] = p.y-5
end
end 
