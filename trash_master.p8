--flower

function create_flower(x,y)
	add(flowers,{
		x = x,
		y = y,
		timer = 0
	})
	sfx(0)
	end
	
function update_flower()
	for f in all(flowers) do
	f.timer +=1
		if f.timer == 13 then
			del(flowers,f)
		end
	end
end

function draw_flower()
	for f in all(flowers) do
		palt(0,true)
		spr(37,f.x, f.y,2,2)
	end
end

